# This file provides example environment variables for configuring OpenWebUI.
# Copy to .env, uncomment (remove the #), and set values as needed.
# Variables are ordered by usage frequency, with commonly modified ones first.
# Categories are used to group related variables for clarity.

# =============================================================================
# Deployment Basics
# =============================================================================
# These control core accessibility and startup behavior.

# TAILSCALE_HOSTNAME
# Applied hostname to a tailscale instance.
# Default: openwebui
# TAILSCALE_HOSTNAME=openwebui

# PORT
# The port OpenWebUI runs on.
# Default: 8080.
#PORT=8080

# WEBUI_URL
# Specifies the URL where OpenWebUI is reachable. Needed for search engine support and OAuth/SSO.
# Default: http://localhost:3000.
#WEBUI_URL=http://localhost:3000

# ENV
# Environment setting.
# Options: dev, prod.
# Default: dev (Backend), prod (Docker).
#ENV=prod

# ENABLE_REALTIME_CHAT_SAVE
# When enabled, saves each chunk of streamed chat data to the database in real time.
# Default: False.
#ENABLE_REALTIME_CHAT_SAVE=false

# CHAT_RESPONSE_STREAM_DELTA_CHUNK_SIZE
# Minimum number of tokens to batch before sending in chat responses.
# Default: 1.
#CHAT_RESPONSE_STREAM_DELTA_CHUNK_SIZE=1

# =============================================================================
# User Management and Authentication
# =============================================================================
# Controls signup, login, and user roles.

# ENABLE_SIGNUP
# Toggles user account creation.
# Default: True.
#ENABLE_SIGNUP=true

# ENABLE_SIGNUP_PASSWORD_CONFIRMATION
# Adds "Confirm Password" field to sign-up page.
# Default: False.
#ENABLE_SIGNUP_PASSWORD_CONFIRMATION=false

# ENABLE_LOGIN_FORM
# Toggles email/password sign-in and "or" elements (with OAuth).
# Default: True.
#ENABLE_LOGIN_FORM=true

# DEFAULT_USER_ROLE
# Default role for new users.
# Options: pending, user, admin.
# Default: pending.
#DEFAULT_USER_ROLE=pending

# ENABLE_OAUTH_SIGNUP
# Enables account creation via OAuth.
# Default: False.
#ENABLE_OAUTH_SIGNUP=false

# ENABLE_OAUTH_PERSISTENT_CONFIG
# Persists OAuth settings in database after first launch.
# Default: True.
#ENABLE_OAUTH_PERSISTENT_CONFIG=true

# OAUTH_MERGE_ACCOUNTS_BY_EMAIL
# Merges OAuth accounts with existing ones by email (unsafe).
# Default: False.
#OAUTH_MERGE_ACCOUNTS_BY_EMAIL=false

# OAUTH_UPDATE_PICTURE_ON_LOGIN
# Updates user profile picture from OAuth on login.
# Default: False.
#OAUTH_UPDATE_PICTURE_ON_LOGIN=false

# OAUTH_SET_ID_TOKEN_COOKIE
# Sets legacy oauth_id_token cookie on OAuth login.
# Default: True.
#OAUTH_SET_ID_TOKEN_COOKIE=true

# WEBUI_AUTH_TRUSTED_EMAIL_HEADER
# Trusted request header for authentication (see SSO docs).
# Default: None.
#WEBUI_AUTH_TRUSTED_EMAIL_HEADER=X-Auth-Email

# WEBUI_AUTH_TRUSTED_NAME_HEADER
# Trusted header for username in trusted email auth (see SSO docs).
# Default: None.
#WEBUI_AUTH_TRUSTED_NAME_HEADER=X-Auth-Name

# WEBUI_AUTH_TRUSTED_GROUPS_HEADER
# Trusted header for comma-separated group memberships (see SSO docs).
# Default: None.
#WEBUI_AUTH_TRUSTED_GROUPS_HEADER=X-Auth-Groups

# =============================================================================
# UI and Localization
# =============================================================================
# Customizes the interface and locale.

# DEFAULT_LOCALE
# Default application locale.
# Default: en.
#DEFAULT_LOCALE=en

# CUSTOM_NAME
# Custom name for WebUI, polls api.openwebui.com for metadata.
# Default: None.
#CUSTOM_NAME=My OpenWebUI

# WEBUI_NAME
# Main WebUI name. Appends (Open WebUI) if overridden.
# Default: Open WebUI.
#WEBUI_NAME=Open WebUI

# PENDING_USER_OVERLAY_TITLE
# Custom title for pending user overlay.
# Default: Empty.
#PENDING_USER_OVERLAY_TITLE=Welcome

# PENDING_USER_OVERLAY_CONTENT
# Custom text for pending user overlay.
# Default: Empty.
#PENDING_USER_OVERLAY_CONTENT=Please wait for approval.

# RESPONSE_WATERMARK
# Custom text included when copying chat messages.
# Default: Empty.
#RESPONSE_WATERMARK=Generated by OpenWebUI

# SHOW_ADMIN_DETAILS
# Shows admin user details in the interface.
# Default: True.
#SHOW_ADMIN_DETAILS=true

# ADMIN_EMAIL
# Admin email shown by SHOW_ADMIN_DETAILS.
# Default: None.
#ADMIN_EMAIL=admin@example.com

# WEBUI_BANNERS
# List of banners to show users. Format: [{"id": "string", "title": "string", "content": "string", "level": "info|warning|error"}].
# Default: [].
#WEBUI_BANNERS=[{"id": "welcome", "title": "Welcome", "content": "Hello!", "level": "info"}]

# =============================================================================
# Models and Features
# =============================================================================
# Controls default models and feature toggles.

# DEFAULT_MODELS
# Default Language Model.
# Default: Empty.
#DEFAULT_MODELS=gpt-3.5-turbo

# ENABLE_CHANNELS
# Enables channel support.
# Default: False.
#ENABLE_CHANNELS=false

# ENABLE_ADMIN_EXPORT
# Allows admins to export data/chats/database.
# Default: True.
#ENABLE_ADMIN_EXPORT=true

# ENABLE_ADMIN_CHAT_ACCESS
# Allows admins to access other users' chats.
# Default: True.
#ENABLE_ADMIN_CHAT_ACCESS=true

# BYPASS_ADMIN_ACCESS_CONTROL
# Treats admins like regular users for workspace access when disabled.
# Default: True.
#BYPASS_ADMIN_ACCESS_CONTROL=true

# ENABLE_USER_WEBHOOKS
# Enables user webhooks.
# Default: True.
#ENABLE_USER_WEBHOOKS=true

# BYPASS_MODEL_ACCESS_CONTROL
# Gives all users access to all models.
# Default: False.
#BYPASS_MODEL_ACCESS_CONTROL=false

# MODELS_CACHE_TTL
# Cache TTL in seconds for model list responses.
# Default: 1.
#MODELS_CACHE_TTL=1

# THREAD_POOL_SIZE
# Thread pool size for FastAPI/AnyIO blocking calls.
# Default: 0.
#THREAD_POOL_SIZE=4

# =============================================================================
# Webhooks and Integrations
# =============================================================================
# For external notifications and tools.

# WEBHOOK_URL
# Webhook for Discord/Slack/Microsoft Teams integration.
# Default: None.
#WEBHOOK_URL=https://hooks.slack.com/services/xxx/yyy/zzz

# =============================================================================
# Image Generation
# =============================================================================
# Configures image generation backends.

# IMAGE_SIZE
# Default image size for generation.
# Default: 512x512.
#IMAGE_SIZE=512x512

# IMAGE_STEPS
# Default iteration steps for image generation (ComfyUI/AUTOMATIC1111).
# Default: 50.
#IMAGE_STEPS=50

# IMAGE_GENERATION_MODEL
# Default model for image generation.
# Default: None.
#IMAGE_GENERATION_MODEL=stable-diffusion-xl

# DEFAULT_IMAGE_PROMPT_GENERATION_PROMPT_TEMPLATE
# Template for generating image prompts.
# Default: None.
#DEFAULT_IMAGE_PROMPT_GENERATION_PROMPT_TEMPLATE=Generate a detailed prompt for an image of {description}

# AUTOMATIC1111_BASE_URL
# URL to AUTOMATIC1111 Stable Diffusion API.
# Default: None.
#AUTOMATIC1111_BASE_URL=http://localhost:7860

# AUTOMATIC1111_API_AUTH
# AUTOMATIC1111 API authentication.
# Default: None.
#AUTOMATIC1111_API_AUTH=token

# AUTOMATIC1111_CFG_SCALE
# Scale for AUTOMATIC1111 inference.
# Default: None.
#AUTOMATIC1111_CFG_SCALE=7.5

# AUTOMATIC1111_SAMPLER
# Sampler for AUTOMATIC1111 inference.
# Default: None.
#AUTOMATIC1111_SAMPLER=euler_a

# AUTOMATIC1111_SCHEDULER
# Scheduler for AUTOMATIC1111 inference.
# Default: None.
#AUTOMATIC1111_SCHEDULER=normal

# COMFYUI_BASE_URL
# URL to ComfyUI image generation API.
# Default: None.
#COMFYUI_BASE_URL=http://localhost:8188

# COMFYUI_API_KEY
# API key for ComfyUI.
# Default: None.
#COMFYUI_API_KEY=secret

# COMFYUI_WORKFLOW
# ComfyUI workflow JSON.
# Default: (Complex JSON structure).
#COMFYUI_WORKFLOW={"workflow": {...}}

# GEMINI_API_BASE_URL
# URL to Gemini API.
# Default: None.
#GEMINI_API_BASE_URL=https://generativelanguage.googleapis.com/v1beta

# GEMINI_API_KEY
# Gemini API key.
# Default: None.
#GEMINI_API_KEY=your_gemini_key

# IMAGES_GEMINI_API_BASE_URL
# URL to Gemini image generation API.
# Default: None.
#IMAGES_GEMINI_API_BASE_URL=https://generativelanguage.googleapis.com/v1beta

# IMAGES_GEMINI_API_KEY
# Gemini API key for image generation.
# Default: None.
#IMAGES_GEMINI_API_KEY=your_gemini_key

# IMAGES_OPENAI_API_BASE_URL
# OpenAI-compatible base URL for DALL-E.
# Default: ${OPENAI_API_BASE_URL}.
#IMAGES_OPENAI_API_BASE_URL=https://api.openai.com/v1

# IMAGES_OPENAI_API_VERSION
# API version for Azure OpenAI image generation.
# Default: ${OPENAI_API_VERSION}.
#IMAGES_OPENAI_API_VERSION=2024-02-15-preview

# IMAGES_OPENAI_API_KEY
# API key for DALL-E image generation.
# Default: ${OPENAI_API_KEY}.
#IMAGES_OPENAI_API_KEY=your_openai_key

# =============================================================================
# OAuth/SSO Configuration
# =============================================================================
# For federated authentication.

# OAUTH_SUB_CLAIM
# Claim for unique user ID from OAuth/OIDC.
# Default: None.
#OAUTH_SUB_CLAIM=sub

# OAUTH_CLIENT_INFO_ENCRYPTION_KEY
# Secret key to encrypt/decrypt OAuth client tokens.
# Default: Falls back to WEBUI_SECRET_KEY.
#OAUTH_CLIENT_INFO_ENCRYPTION_KEY=your_secret_key

# OAUTH_SESSION_TOKEN_ENCRYPTION_KEY
# Secret key to encrypt/decrypt OAuth session tokens.
# Default: Falls back to WEBUI_SECRET_KEY.
#OAUTH_SESSION_TOKEN_ENCRYPTION_KEY=your_secret_key

# =============================================================================
# Advanced and Development
# =============================================================================
# Less commonly changed settings.

# WEBUI_BUILD_HASH
# Git SHA for build identification.
# Default: dev-build.
#WEBUI_BUILD_HASH=dev-build

# ENABLE_PERSISTENT_CONFIG
# Treats PersistentConfig vars as regular if False.
# Default: True.
#ENABLE_PERSISTENT_CONFIG=true

# USE_CUDA_DOCKER
# Builds Docker with NVIDIA CUDA for GPU Whisper.
# Default: False.
#USE_CUDA_DOCKER=false

# HTTP_PROXY
# Proxy for unencrypted HTTP requests.
# Default: None.
#HTTP_PROXY=http://proxy.example.com:8080

# HTTPS_PROXY
# Proxy for encrypted HTTPS requests.
# Default: None.
#HTTPS_PROXY=https://proxy.example.com:8443